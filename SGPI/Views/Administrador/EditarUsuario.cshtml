
@{
    ViewData["Title"] = "EditarUsuario";
 }   

<body>
    <h1 class="col-lg-6 offset-lg-5 ">Editar Usuario</h1>
    <nav class=" navbar navbar-expand-sm bg-dark navbar-dark  justify-content-center" style="color:#DBF9FC">
        <ul class="navbar-nav">
            <li class="nav-item active">
                <a class="nav-link" href="/Administrador/CrearUsuario">Crear Usuario</a>
                
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Administrador/BuscarUsuario">Buscar Usuario</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Administrador/EliminarUsuario">Eliminar Usuario</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Administrador/EditarUsuario">Editar Usuario </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Administrador/Reportes">Reportes</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/Administrador/Login" style="color:blue;"> Cerrar Sesion</a>

            </li>
        </ul>
    </nav>
    <br />
    <br />
    <div>

        <div class="row">
            <div class="col-sm-4 justify-content-md-center align-items-center ">
                <h4> Tipo de Documento </h4>
                <select class="form-select col-sm-12" aria-label="Default select example">
                     <option>          </option>
                    <option> Cedula De Ciudadania </option>
                    <option> Documento Extranjeria </option>
                    <option> Tarjeta De Identidad </option>
                    <option> Nit </option>
                    <option> Pasaporte </option>
                </select>
            </div>
            <br />

            <div class="col-sm-4 justify-content-md-center align-items-center">
                <form>
                    <h4> Numero de Documento </h4>
                    <div class="col ">
                        <input type="text" class="form-control col-sm-3" id="Documentotxt"  name="Documento">
                    </div>
                </form>
            </div>

            <div class="col-sm-4 justify-content-md-center align-items-center">
                <form>
                    <h4> Género </h4>
                    <div class="col ">
                        <select class="form-select col-sm-12" aria-label="Default select example">
                             <option>          </option>
                            <option> Masculino </option>
                            <option> Femenino </option>
                            <option> Otro </option>
                        </select>
                    </div>
                </form>
            </div>
            <br />
            <br />
            <br />



        </div>
        <br />

        <div class="row">
            <div class="col-sm-4 justify-content-md-center align-items-center h-100">
                <form>
                    <h4> Nombre </h4>
                    <div class="col ">
                        <input type="text" class="form-control col-sm-3" id="Nombretxt"  name="Nombre">
                    </div>
                </form>
            </div>

            <div class="col-sm-4 justify-content-md-center align-items-center h-100">
                <form>
                    <h4> Primer Apellido </h4>
                    <div class="col ">
                        <input type="text" class="form-control col-sm-3" id="Apellido1txt"  name="Apellido1">
                    </div>
                </form>
            </div>

            <div class="col-sm-4 justify-content-md-center align-items-center h-100">
                <form>
                    <h4> Segundo Apellido </h4>
                    <div class="col ">
                        <input type="text" class="form-control col-sm-3" id="Apellido2"  name="Apellido2">
                    </div>
                </form>
            </div>
        </div>
        <br />

        <div class="row">
            <div class="col-sm-4 justify-content-md-center align-items-center h-100">
                <form>
                    <h4> Correo Electronico </h4>
                    <div class="col ">
                        <input type="text" class="form-control col-sm-3" id="Correotxt"  name="Correo">
                    </div>
                </form>
            </div>

            <div class="col-sm-4 justify-content-md-center align-items-center ">
                <h4> Roles </h4>
                <select class="form-select col-sm-12" aria-label="Default select example">
                     <option>          </option>
                    <option> Administrador </option>
                    <option> Coordinador </option>
                    <option> Estudiante </option>
                </select>
            </div>

            <div class="col-sm-4 justify-content-md-center align-items-center ">
                <h4> Programas </h4>
                <select class="form-select col-sm-12" aria-label="Default select example">
                    <option>          </option>
                    <option> Maestría en Gestión de La  Información </option>
                    <option>  Especialización en Seguridad de la Información </option>
                    <option> Maestría en Medio Ambiente </option>
                </select>
            </div>
        </div>
        <br />
        <br />


        <div class="col-lg-6 offset-lg-5">
            <button type="button" class="btn btn-dark">Editar Usuario</button>
        </div>
    </div>
</body>

